  "repository": "github:la-jarre-a-son/quicongen",
  "homepage": "https://la-jarre-a-son.github.io/quicongen/",
  "keywords": [
    "icons",
    "generator",
    "streamdeck"
  ],
  "license": "ISC",
  "dependencies": {
    "classnames": "^2.3.2",
    "core-js": "^2.5.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-react": "^7.18.6",
    "eslint": "^8.24.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "parcel": "^2.7.0",
    "posthtml-expressions": "^1.10.0",
    "prettier": "^2.7.1",
    "process": "^0.11.10",
    "rimraf": "^3.0.2"
  }
